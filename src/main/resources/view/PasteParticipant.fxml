<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane style="-fx-background-color: orange;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="30.0" layoutY="30.0" text="Paste Participants in the textbox provided ~" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0" />
      <GridPane AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="50.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="49.0" />
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="2.0" />
            <ColumnConstraints hgrow="SOMETIMES" percentWidth="49.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TextArea fx:id="tbPasteParticipants" promptText="Copy and paste the text from discord" />
            <ListView fx:id="listView" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
